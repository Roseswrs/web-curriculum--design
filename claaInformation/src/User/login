<script type="text/javascript">
	function check() {
		var form = document.getElementById("form1");

		var name = document.getElementById("name").value;
		var password = document.getElementById("password").value;
		if (name == "" || password == "") {
			alert("用户名和密码均不能为空，请重新填写");
			return false;
		} else {
			form.submit();
		}
	}
</script>

